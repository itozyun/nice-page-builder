{
    "name": "nice-page-builder",
    "displayName": "Nice Page Builder",
    "description": "Static site generator on VSCode Extension.",
    "version": "0.1.0",
    "publisher": "itozyun",
    "icon": "icon.png",
    "galleryBanner": {
        "color": "#72C8FA",
        "theme": "dark"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/itozyun/nice-page-builder.git"
    },
    "bugs": {
        "url": "https://github.com/itozyun/nice-page-builder/issues"
    },
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.nicePageBuilder"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.nicePageBuilder",
                "title": "Build Nice Page"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "dependencies": {
        "cheerio": "^1.0.0-rc.3",
        "lodash": "^4.17.15",
        "minimatch": "^3.0.4"
    },
    "devDependencies": {
        "vscode": "^0.11.0"
    }
}