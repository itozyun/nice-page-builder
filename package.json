{
    "name": "nice-page-builder",
    "displayName": "Nice Page Builder",
    "description": "Static site generator on VSCode Extension.",
    "version": "0.0.8",
    "publisher": "itozyun",
    "icon": "icon.png",
    "galleryBanner": {
        "color": "#72C8FA",
        "theme": "dark"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/itozyun/nice-page-builder.git"
    },
    "bugs": {
        "url": "https://github.com/itozyun/nice-page-builder/issues"
    },
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.nicePageBuilder"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.nicePageBuilder",
            "title": "Build Nice Page"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "dependencies" : {
        "minimatch": ">0.0.0"
    },
    "devDependencies": {
        "vscode": "^0.11.0"
    }
}